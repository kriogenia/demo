{
  "query": {
    "bool": {
      "must": {
        "match": {
          "original_title": "the"
        }
      },
      "filter": [
        {
          "range": {
            "start_year": {
              "gte": "2005",
              "lte": "2020",
              "format": "YYYY"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "genres": {
      "terms": {
        "field": "genres"
      }
    },
    "type": {
      "terms": {
        "field": "type"
      }
    },
    "year": {
      "date_range": {
        "field": "start_year",
        "ranges": [
          {
            "from": "2010",
            "to": "2020"
          }
        ]
      }
    }
  }
}