{
  "query": {
    "bool": {
      "must": {
        "match": {
          "original_title": "the"
        }
      },
      "filter": [
        {
          "range": {
            "start_year": {
              "gte": "2015",
              "lte": "2020",
              "format": "YYYY"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "genres": {
      "terms": {
        "field": "genres"
      }
    },
    "type":{
      "terms": {
        "field": "type"
      }
    }
  }
}